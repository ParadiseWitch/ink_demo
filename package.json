{
	"name": "ink-copy",
	"version": "0.0.0",
	"license": "MIT",
	"bin": {
		"ink-copy": "dist/cli.js"
	},
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"build": "tsc",
		"start": "tsc && node dist/cli.js",
		"debug": "tsc && cross-env DEV=true node dist/cli.js ",
		"pretest": "npm run build",
		"test": "tsc && node dist/test.js",
		"clear": "rmdir /s/q dist & mkdir dist"
	},
	"files": [
		"dist/cli.js"
	],
	"dependencies": {
		"dedent": "^0.7.0",
		"fdir": "^5.0.0",
		"fs-extra": "^9.1.0",
		"ink": "^3.0.9",
		"ink-markdown": "^1.0.1",
		"ink-select-input": "^4.2.0",
		"ink-spinner": "^4.0.1",
		"ink-tab": "^4.1.0",
		"meow": "^9.0.0",
		"react": "^17.0.1"
	},
	"devDependencies": {
		"@ava/typescript": "^1.1.1",
		"@sindresorhus/tsconfig": "^0.9.0",
		"@types/dedent": "^0.7.0",
		"@types/fs-extra": "^9.0.7",
		"@types/react": "^17.0.2",
		"ava": "^3.15.0",
		"chalk": "^4.1.0",
		"eslint-config-xo-react": "^0.24.0",
		"eslint-plugin-react": "^7.22.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"ink-testing-library": "^2.1.0",
		"typescript": "^4.2.2",
		"xo": "^0.38.1"
	},
	"ava": {
		"typescript": {
			"extensions": [
				"tsx"
			],
			"rewritePaths": {
				"source/": "dist/"
			}
		}
	},
	"xo": {
		"extends": "xo-react",
		"rules": {
			"react/prop-types": "off"
		}
	}
}
